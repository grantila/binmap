{
  "name": "binmap",
  "description": "Ordered Map (ES Map compatible)",
  "license": "MIT",
  "version": "0.0.0-development",
  "author": "Gustaf Räntilä <g.rantila@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/binmap"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "build": "scripts/build.sh",
    "mocha": "node_modules/.bin/mocha --bail --check-leaks build-test",
    "test": "node_modules/.bin/nyc npm run mocha",
    "buildtest": "npm run build && npm run test",
    "coverage": "node_modules/.bin/nyc report --reporter=html",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "cz": "git-cz"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "keywords": [
    "map",
    "ordered",
    "sorted",
    "binary",
    "tree",
    "red-black"
  ],
  "devDependencies": {
    "@types/bintrees": "1.x",
    "@types/chai": "4.x",
    "@types/mocha": "5.x",
    "@types/sinon": "5.x",
    "chai": "4.x",
    "coveralls": "3.x",
    "cz-conventional-changelog": "2.x",
    "mocha": "5.x",
    "nyc": "13.x",
    "pre-commit": "1.x",
    "semantic-release": "15.x",
    "sinon": "7.x",
    "source-map-support": "0.x",
    "travis-deploy-once": "5.x",
    "typescript": "3.x"
  },
  "dependencies": {
    "bintrees": "1.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
